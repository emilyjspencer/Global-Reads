<h1 align="center">All Users</h1>
<div align="center">

<% @users.each do |user| %>
    <ul class="listing">
     <div class="row">
      <div class="well col-md-4 col-md-offset-4">
       <li><%= link_to gravatar_for(user), user_path(user) %></li>
       <li class="review-title"><%= link_to user.username, user_path(user) %></li>
       <li><small><%= pluralize(user.review.count, "review") if user.reviews %></small></li>
      </div>
     </div>
    </ul>
    <%= will_paginate %>
   <% end %>